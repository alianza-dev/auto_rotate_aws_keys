<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.tsmith.aws-rotate-keys</string>
    <key>KeepAlive</key>
    <false/>
    <key>RunAtLoad</key>
    <false/>
    <key>Program</key>
    <string>/usr/local/bin/aws-rotate-keys</string>
    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>10</integer>
        <key>Minute</key>
        <integer>03</integer>
        <key>Day</key>
        <integer>30</integer>
    </dict>

    <key>StandardErrorPath</key>
    <string>/tmp/aws-rotate-keys.err</string>

  	<key>StandardOutPath</key>
  	<string>/tmp/aws-rotate-keys.out</string>
</dict>
</plist>
